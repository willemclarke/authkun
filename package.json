    "pg": "^8.7.3",
    "slonik": "^27.1.s0",
    "uuid": "^8.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
